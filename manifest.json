{
  "manifest_version": 3,
  "name": "Anime Dashboard",
  "version": "1.0.0",
  "description": "A beautiful new tab dashboard with anime wallpapers and inspirational anime quotes",
  "author": "Your Name",
  "homepage_url": "https://github.com/yourusername/anime-dashboard-extension",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Anime Dashboard"
  },
  "web_accessible_resources": [{
    "resources": [
      "images/*",
      "icons/*"
    ],
    "matches": ["<all_urls>"]
  }]
}